{
  "name": "peer-pass",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=14.x.x"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:tsc": "tsc  --project ./tsconfig.json",
    "start": "npm-run-all --parallel start:*",
    "start:createRoom": "npx functions-framework --target=createRoom --signature-type=http --port=3000",
    "start:connectToRoom": "npx functions-framework --target=connectToRoom --signature-type=http --port=3001",
    "deploy": "npm-run-all deploy:setup --parallel deploy:createRoom deploy:connectToRoom",
    "deploy:setup": "gcloud config set project peerpass-315217",
    "deploy:createRoom": "gcloud functions deploy createRoom --runtime nodejs14 --trigger-http --allow-unauthenticated --security-level=secure-always",
    "deploy:connectToRoom": "gcloud functions deploy connectToRoom --runtime nodejs14 --trigger-http --allow-unauthenticated --security-level=secure-always"
  },
  "author": "Richie Casto (https://github.com/rcasto)",
  "license": "MIT",
  "devDependencies": {
    "@google-cloud/functions-framework": "1.7.1",
    "@types/express": "4.17.12",
    "npm-run-all": "4.1.5",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "@google-cloud/storage": "5.8.5"
  }
}
